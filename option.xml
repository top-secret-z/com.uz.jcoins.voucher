<?xml version="1.0" encoding="UTF-8"?>
<data xmlns="http://www.woltlab.com" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.woltlab.com http://www.woltlab.com/XSD/2019/option.xsd">
    <import>
        <categories>
            <category name="user.jcoins.voucher">
                <parent>user.jcoins</parent>
            </category>
        </categories>

        <options>
            <option name="module_jcoins_voucher">
                <categoryname>user.jcoins.voucher</categoryname>
                <optiontype>boolean</optiontype>
                <defaultvalue>1</defaultvalue>
            </option>

            <option name="jcoins_voucher_items_per_page">
                <categoryname>user.jcoins.voucher</categoryname>
                <optiontype>integer</optiontype>
                <defaultvalue>6</defaultvalue>
                <minvalue>2</minvalue>
                <maxvalue>50</maxvalue>
            </option>

            <option name="jcoins_voucher_log_delete">
                <categoryname>user.jcoins.voucher</categoryname>
                <optiontype>integer</optiontype>
                <defaultvalue>30</defaultvalue>
                <minvalue>0</minvalue>
                <maxvalue>365</maxvalue>
                <suffix>days</suffix>
            </option>
        </options>
    </import>
</data>
